{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/"
      }
    ],
    "env": {
      "AIPROXY_BASE_URL": "https://aiproxy.sanand.workers.dev/openai/v1"
    }
  }